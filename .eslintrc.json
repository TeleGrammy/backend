{
  "extends": [
    "airbnb-base",
    "plugin:node/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-console": "warn",
    "no-unused-vars": ["error", {"argsIgnorePattern": "req|res|next|err"}],
    "no-underscore-dangle": "off",
    "func-names":"off"
  }
}
